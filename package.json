{
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "argon2": "^0.43.0",
    "axios": "^1.10.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "moment-timezone": "^0.6.0",
    "playwright": "^1.53.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "winston": "^3.17.0"
  },
  "name": "playwright-typescript-automation-framework",
  "version": "1.0.0",
  "description": "This repository contains the Playwright Test Automation Framework, built using Playwright and TypeScript. Designed with scalability, security, and maintainability in mind, it supports end-to-end UI, API, and database test automation",
  "main": "index.js",
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@types/node": "^24.0.3",
    "cspell": "9.1.1",
    "eslint": "9.29.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.5.0",
    "ortoni-report": "3.0.2",
    "prettier": "3.6.0"
  },
  "scripts": {
    "type:check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "check:all": "npm run type:check && npm run lint",
    "pretest:encryption:dev": "npm run check:all",
    "pretest:encryption:uat": "npm run check:all",
    "pretest:ui:dev": "npm run check:all",
    "pretest:ui:uat": "npm run check:all",
    "pretest:rerun:dev": "npm run check:all",
    "pretest:rerun:uat": "npm run check:all",
    "test:api:dev": "cross-env ENV=dev SKIP_BROWSER_INIT=true npx playwright test tests/encryption",
    "test:api:uat": "cross-env ENV=uat SKIP_BROWSER_INIT=true npx playwright test tests/encryption",
    "test:ui:dev": "cross-env ENV=dev npx playwright test tests/ui --project=chromium",
    "test:ui:uat": "cross-env ENV=uat npx playwright test tests/ui",
    "test:rerun:dev": "cross-env ENV=dev npx playwright test --last-failed",
    "test:rerun:uat": "cross-env ENV=uat npx playwright test --last-failed",
    "ui": "npx playwright test --ui",
    "record": "npx playwright codegen",
    "report": "npx playwright show-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinugotshifhiwa4/playwright-typescript-automation-framework.git"
  },
  "keywords": [
    "playwright",
    "typescript",
    "automation",
    "e2e",
    "ui",
    "api",
    "database",
    "integration",
    "unit",
    "sanity",
    "regression",
    "allure-report",
    "reporting",
    "logging",
    "winston",
    "dotenv",
    "cross-env",
    "eslint",
    "prettier",
    "argon2"
  ],
  "author": "Tshifhiwa Sinugo",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/sinugotshifhiwa4/playwright-typescript-automation-framework/issues"
  },
  "homepage": "https://github.com/sinugotshifhiwa4/playwright-typescript-automation-framework#readme"
}
